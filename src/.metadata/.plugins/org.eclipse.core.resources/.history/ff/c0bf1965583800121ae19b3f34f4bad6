import java.util.Collection;

public class Board {
	Collection<Ship> ships;
	
	public Deck getDeckAt(Location location) {
		for (Ship ship : ships) {
  			for (Deck deck : ship.getDecks()) {
  				if (deck.getLocation().equals(location)) {
  					return deck;
  				}
  			}
  		}	
	}
	
  	public CellType getCellType(int x, int y) {
  		
  	}
}
